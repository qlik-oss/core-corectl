# EditorConfig is awesome: http://EditorConfig.org

# Top-most EditorConfig file
root = true

# Source files
[*]
charset = utf-8
end_of_line = lf
insert_final_newline = true
indent_style = space
indent_size = 2

[Makefile]
indent_style = tab

[*.golden]
insert_final_newline = false

[*.go]
indent_style = tab
